{
    "name": "mintunitish/laravel-log-enhancer",
    "description": "Adds more data to logs for easy debugging.",
    "type": "library",
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "^5.6",
        "monolog/monolog": "^1.23"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nitish Kumar",
            "email": "mintu.nitish@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "~3.6.0"
    },
    "autoload": {
        "psr-4": {
            "AwesomePeople\\LaravelLogEnhancer\\": "src/",
            "AwesomePeople\\LaravelLogEnhancer\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers" : [
                "AwesomePeople\\LaravelLogEnhancer\\LaravelLogEnhancerServiceProvider"
            ]
        }
    }
}
